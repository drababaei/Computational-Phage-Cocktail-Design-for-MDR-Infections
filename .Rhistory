# Prepare phage property data
selected_phages <- phage_db[phage_db$phage_id %in% enhanced_cocktail, ]
properties_melt <- melt(selected_phages[, c("phage_id", "efficacy", "host_range", "virulence_genes")],
id.vars = "phage_id")
properties_melt$variable <- factor(properties_melt$variable,
levels = c("efficacy", "host_range", "virulence_genes"),
labels = c("Efficacy", "Host Range", "Virulence Genes"))
# Normalize values
properties_melt$value <- ifelse(
properties_melt$variable == "Efficacy",
properties_melt$value * 100,
properties_melt$value
)
